[
  {
    "description":"这是一个带header信息头的post请求",
    "request":{
      "uri":"/post/with/header",
      "method":"post",
      "headers":{
        "content-type":"application/json"
      },
      "json":{
        "name":"huhansan",
        "age":"18"
      }
    },
    "response":{
      "status":200,
      "json":{
        "huhansan":"success",
        "status":"1"
      }
    }
  }
]